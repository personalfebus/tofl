    add_compile_options(-DDEBUG=0)
    IF(tol_lab5)
        add_compile_options(-DDEBUG=1)
        message(ababab)
    ENDIF()
ELSE(sdfsdf)
    set(CMAKE_CXX_STANDARD 14)
