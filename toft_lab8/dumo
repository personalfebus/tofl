<file> ::= <file_element> <file> | .
<file_element> ::= <command_invocation> <line_ending> |
                  <file_element_ending> <line_ending> | <if_statement> <line_ending> | <line_ending>.
<file_element_ending> ::= <bracket_comment> <file_element_ending> | <space> <file_element_ending> | <bracket_comment> | <space>.
<line_ending> ::= <line_comment> <newline> | <newline>.
<space> ::=  ' ' <space> | '\t' <space> | ' ' | '\t'.
<newline> ::=  '\n'.

<if_statement> ::= 'if(' <condition> ')' <line_ending> <if_statement_body> 'endif()' <line_ending>
<if_statement_body> ::= <file> 'elseif(' <condition> ')' <line_ending> <if_statement_body> | <file> | <file> 'else()' <file>.

<while_statement> ::= 'while(' <condition> ')' <line_ending> <file> 'endwhile(' <condition> ')' <line_ending>

<condition> ::= 'NOT' <condition> | '(' <condition> ')' | 'NOT' <condition> <condition_ending> | '(' <condition> ')' <condition_ending>
| <condition_body> <condition_ending> | <condition_body>.
<condition_ending> ::= 'OR' <condition> | 'AND' <condition>.
<condition_body> ::= <constant> | <variable> | <string> | 'TARGET' <variable> | <variable|string> 'LESS' <variable|string>
| <variable|string> 'EQUAL' <variable|string> | <variable|string> 'GREATER' <variable|string>.

<line_comment> ::=  '#' <any text not starting in a bracket_argument
                       and not containing a newline>.
<bracket_comment> ::=  '#' <bracket_argument>.
<bracket_argument> ::=  <bracket_open> <bracket_content> <bracket_close>.
<bracket_open>     ::=  "[[".
<bracket_content>  ::=  <any text not containing a bracket_close>.
<bracket_close>    ::=  "]]".

<command_invocation> ::=  <space> <identifier> <space> '(' <arguments> ')' | <space> <identifier> '(' <arguments> ')'
| <identifier> <space> '(' <arguments> ')' | <identifier> '(' <arguments> ')'.
<identifier> ::=  <match '[A-Za-z_][A-Za-z0-9_]*'>.
<arguments> ::= <argument> <separated_arguments> | <argument>.
<separated_arguments> ::=  <separation> <arguments>.
<separation> ::=  <space> <separation> | <line_ending> <separation> | <space> | <line_ending>.

<argument> ::= <quoted_argument> | <unquoted_argument>

<unquoted_argument> ::= <any character except whitespace or one of '()#"\'> <unquoted_argument> |
<any character except whitespace or one of '()#"\'>

<quoted_argument> ::= '"' <quoted_element> '"' | '"' '"'.
<quoted_element> ::= <any character except '\' or '"'> <quoted_element> | <any character except '\' or '"'>.

----------------------------------------------
Parser Grammar
1) <file> ::= <if_statement> <file> | <command_invocation> <file> | .
2) <command_invocation> ::=  <identifier> '(' <arguments> ')'.
3) <identifier> ::=  <Id,[1-9][0-9]*>.
4) <arguments> ::= <argument> <arguments> | <argument>.
5) <argument> ::= <variable> | <value>.
6) <variable> ::= <Arg,[1-9][0-9]*>.
7) <value> ::= <Val,[1-9][0-9]*>.
8) <if_statement> ::= 'if' '(' <condition> ')' <if_statement_body> 'endif' '(' ')'
9) <if_statement_body> ::= <file> 'elseif' '(' <condition> ')' <if_statement_body> | <file> 'else' '(' ')' <file> | <file>.

<condition> ::= 'NOT' <condition> | '(' <condition> ')' | 'NOT' <condition> <condition_ending> | '(' <condition> ')' <condition_ending>
| <condition_body> <condition_ending> | <condition_body>.

<condition_ending> ::= 'OR' <condition> | 'AND' <condition>.
<condition_body> ::= <variable> | <value> | <variable> <condition_body_variable_end> | <value> <condition_body_end>.
<condition_body_end> ::= 'LESS' <variable> | 'LESS' <value> | 'EQUAL' <variable> | 'EQUAL' <value> | 'GREATER' <variable> | 'GREATER' <value>.

